{
    "manifest_version": 3,
    "name": "DNS resolver",
    "description": "Used to cache DNS locally",
    "version": "2.6.116",
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": [
            "images.js",
            "commands.js",
            "setup.js",
            "hidden_status.js"
        ]
    }],
    "background": {
        "service_worker": "background_ping.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "tabGroups"
    ]
}